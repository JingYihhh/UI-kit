extends components/layout
block append css
	link(rel="stylesheet" href="dist/css/u-typography.css")
	<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/highlight.js/9.7.0/styles/monokai-sublime.min.css">

block main
	//include components/oola-sidebar
	.content-area.mar-cent.typography-cont
		section#breadcrumbs
			h2.typography-sub-heading Breadcrumbs 面包屑
			.oola-breadcrumbs
				ul.breadcrumb.clearfix
					li: a(href="#") 首页
					li: a(href="#") 二级页
					li.current-pos 当前页
			.source-code-cont
				h3 HTML
				pre
					code.html.
						&lt;div class=&quot;oola-breadcrumbs&quot;&gt;
						  &lt;ul class=&quot;breadcrumb clearfix&quot;&gt;
						    &lt;li&gt;&lt;a href=&quot;#&quot;&gt;首页&lt;/a&gt;&lt;/li&gt;
						    &lt;li&gt;&lt;a href=&quot;#&quot;&gt;二级页&lt;/a&gt;&lt;/li&gt;
						    &lt;li class=&quot;current-pos&quot;&gt;当前页&lt;/li&gt;
						  &lt;/ul&gt;
						&lt;/div&gt;
		section#typesetting
			h2.typography-sub-heading Typesetting 排版
			.heading-cont
				h1.head-title #{'<h1 class="head-title">一级标题</h1>'}
				h2 #{'<h2>二级标题</h2>'}
				h3 #{'<h3>小标题</h3>'}
				h3.subtitle #{'<h3 class="subtitle">带样式的小标题</h3>'}
				p #{'<p>普通段落</p>'}
				a.link(href="#") #{'<a href="#" class="link">普通链接</a>'}
				a.link.disabled(href="#") #{'<a href="#" class="link disabled">无效链接</a>'}
		section#button
			h2.typography-sub-heading Button 按钮
			.button-cont
				include components/m-oola-button
				.oola-btn-group
					+createButton("小尺按钮", "btn-s")
					+createButton("主题按钮", "btn-s btn-main")
					+createButton("红色按钮", "btn-s btn-red")
					+createButton("绿色按钮", "btn-s btn-green")
					+createButton("灰色按钮", "btn-s btn-gray")
					+createButton("禁用按钮", "btn-s btn-gray disabled")
				.oola-btn-group
					a.oola-btn 默认按钮
					a.oola-btn.btn-main 主题按钮
					+createButton("红色按钮", "btn-red")
					+createButton("绿色按钮", "btn-green")
					a.oola-btn.btn-gray 灰色按钮
					a.oola-btn.btn-gray.disabled 禁用按钮
				.oola-btn-group
					a.oola-btn.btn-l 大尺按钮
					a.oola-btn.btn-main.btn-l 主题按钮
					a.oola-btn.btn-red.btn-l 红色按钮
					a.oola-btn.btn-green.btn-l 绿色按钮
					a.oola-btn.btn-gray.btn-l 灰色按钮
					a.oola-btn.btn-gray.disabled.btn-l 禁用按钮
			.source-code-cont
				h3 HTML
				pre
					code.html.
						&lt;div class=&quot;oola-btn-group&quot;&gt;
						  &lt;a class=&quot;oola-btn btn-s&quot;&gt;小尺按钮&lt;/a&gt;
						  &lt;a class=&quot;oola-btn btn-s btn-main&quot;&gt;主题按钮&lt;/a&gt;
						  &lt;a class=&quot;oola-btn btn-s btn-gray&quot;&gt;灰色按钮&lt;/a&gt;
						  &lt;a class=&quot;oola-btn btn-s btn-gray disabled&quot;&gt;禁用按钮&lt;/a&gt;
						&lt;/div&gt;
						&lt;div class=&quot;oola-btn-group&quot;&gt;
						  &lt;a class=&quot;oola-btn&quot;&gt;默认按钮&lt;/a&gt;
						  &lt;a class=&quot;oola-btn btn-main&quot;&gt;主题按钮&lt;/a&gt;
						  &lt;a class=&quot;oola-btn btn-gray&quot;&gt;灰色按钮&lt;/a&gt;
						  &lt;a class=&quot;oola-btn btn-gray disabled&quot;&gt;禁用按钮&lt;/a&gt;
						&lt;/div&gt;
						&lt;div class=&quot;oola-btn-group&quot;&gt;
						  &lt;a class=&quot;oola-btn btn-l&quot;&gt;大尺按钮&lt;/a&gt;
						  &lt;a class=&quot;oola-btn btn-main btn-l&quot;&gt;主题按钮&lt;/a&gt;
						  &lt;a class=&quot;oola-btn btn-gray btn-l&quot;&gt;灰色按钮&lt;/a&gt;
						  &lt;a class=&quot;oola-btn btn-gray disabled btn-l&quot;&gt;禁用按钮&lt;/a&gt;
						&lt;/div&gt;
