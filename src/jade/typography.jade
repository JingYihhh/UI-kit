		section#notification
			h2.typography-sub-heading Notification 信息提示（弹框）
			.notice-cont
				p.notice-block 文本提示基类class="notice-block"
				p.notice-block.info-notice class="notice-block info-notice"
				p.notice-block.success-notice class="notice-block success-notice"
				p.notice-block.warning-notice class="notice-block warning-notice"
				p.notice-block.error-notice class="notice-block error-notice"
			.source-code-cont
				h3 HTML
				pre
					code.html.
						&lt;div class=&quot;notice-cont&quot;&gt;
						  &lt;p class=&quot;notice-block&quot;&gt;文本提示基类class=&quot;notice-block&quot;&lt;/p&gt;
						  &lt;p class=&quot;notice-block info-notice&quot;&gt;class=&quot;notice-block info-notice&quot;&lt;/p&gt;
						  &lt;p class=&quot;notice-block success-notice&quot;&gt;class=&quot;notice-block success-notice&quot;&lt;/p&gt;
						  &lt;p class=&quot;notice-block warning-notice&quot;&gt;class=&quot;notice-block warning-notice&quot;&lt;/p&gt;
						  &lt;p class=&quot;notice-block error-notice&quot;&gt;class=&quot;notice-block error-notice&quot;&lt;/p&gt;
						&lt;/div&gt;
		section#form
			h2.typography-sub-heading Form 表单
			include components/m-oola-form
			form.form-horizontal
				+createFormGroup("手机号：", false)
					p.form-control-static 158****7117
				+createFormGroup("姓名：", false, true, "姓名不能为空！")
					input.form-control(placeholder="请输入您的姓名")
				+createFormGroup("邮箱：", false, false, "邮箱不能为空！")
					.input-group
						input.form-control(type="text" placeholder="请输入您的邮箱")
						span.input-group-addon
							span.glyphicon.glyphicon-envelope
				+createFormGroup("工作：", false, false)
					+createSltControl(["Web前端", "IOS开发", "Android开发"])
				+createFormGroup("性别：", false)
					+createRadio("sex", "male", "男", true)
					+createRadio("sex", "female", "女", true)
				+createFormGroup("爱好：", false)
					+createCheckbox("football", "足球", true)
					+createCheckbox("basketball", "篮球", true)
					+createCheckbox("music", "音乐", true)
					+createCheckbox("draw", "画画", true)
				+createFormGroup("简介：", false, false, "简介不能为空！")
					textarea.form-control(placeholder="请输入自我简介")
				+createFormGroup("", true, true, "请勾选同意协议！")
					+createCheckbox("agree", "同意XXX协议", false)
				+createFormGroup("",true, false)
					button.oola-btn(type="submit" disabled) 提交
			.source-code-cont
				h3 HTML
				pre
					code.html.
						&lt;form class=&quot;form-horizontal&quot;&gt;
						  &lt;div class=&quot;form-group&quot;&gt;
						    &lt;label class=&quot;col-sm-2 control-label&quot;&gt;手机号：&lt;/label&gt;
						    &lt;div class=&quot;col-sm-10&quot;&gt;
						      &lt;p class=&quot;form-control-static&quot;&gt;158****7117&lt;/p&gt;
						    &lt;/div&gt;
						  &lt;/div&gt;
						  &lt;div class=&quot;form-group&quot;&gt;
						    &lt;label class=&quot;col-sm-2 control-label&quot;&gt;姓名：&lt;/label&gt;
						    &lt;div class=&quot;col-sm-10&quot;&gt;
						      &lt;input placeholder=&quot;请输入您的姓名&quot; class=&quot;form-control&quot;&gt;
						      &lt;div class=&quot;help-block&quot;&gt;姓名不能为空！&lt;/div&gt;
						    &lt;/div&gt;
						  &lt;/div&gt;
						  &lt;div class=&quot;form-group&quot;&gt;
						    &lt;label class=&quot;col-sm-2 control-label&quot;&gt;邮箱：&lt;/label&gt;
						    &lt;div class=&quot;col-sm-10 has-error&quot;&gt;
						      &lt;div class=&quot;input-group&quot;&gt;
						        &lt;input type=&quot;text&quot; placeholder=&quot;请输入您的邮箱&quot; class=&quot;form-control&quot;&gt;&lt;span class=&quot;input-group-addon&quot;&gt;&lt;span class=&quot;glyphicon glyphicon-envelope&quot;&gt;&lt;/span&gt;&lt;/span&gt;
						      &lt;/div&gt;
						      &lt;div class=&quot;help-block&quot;&gt;邮箱不能为空！&lt;/div&gt;
						    &lt;/div&gt;
						  &lt;/div&gt;
						  &lt;div class=&quot;form-group&quot;&gt;
						    &lt;label class=&quot;col-sm-2 control-label&quot;&gt;工作：&lt;/label&gt;
						    &lt;div class=&quot;col-sm-10&quot;&gt;
						      &lt;select class=&quot;form-control&quot;&gt;
						        &lt;option&gt;IOS开发&lt;/option&gt;
						        &lt;option&gt;Web前端开发Web前端开发Web前端开发Web前端开发Web前端开发Web前端开发&lt;/option&gt;
						        &lt;option&gt;Android开发&lt;/option&gt;
						      &lt;/select&gt;
						    &lt;/div&gt;
						  &lt;/div&gt;
						  &lt;div class=&quot;form-group&quot;&gt;
						    &lt;label class=&quot;col-sm-2 control-label&quot;&gt;性别：&lt;/label&gt;
						    &lt;div class=&quot;col-sm-10&quot;&gt;
						      &lt;label class=&quot;radio-inline&quot;&gt;
						        &lt;input type=&quot;radio&quot; name=&quot;sex&quot; value=&quot;female&quot; id=&quot;female&quot; checked=&quot;checked&quot;&gt;
						        &lt;label for=&quot;female&quot;&gt;&lt;/label&gt;女
						      &lt;/label&gt;
						      &lt;label class=&quot;radio-inline&quot;&gt;
						        &lt;input type=&quot;radio&quot; name=&quot;sex&quot; value=&quot;male&quot; id=&quot;male&quot;&gt;
						        &lt;label for=&quot;male&quot;&gt;&lt;/label&gt;男
						      &lt;/label&gt;
						    &lt;/div&gt;
						  &lt;/div&gt;
						  &lt;div class=&quot;form-group&quot;&gt;
						    &lt;label class=&quot;col-sm-2 control-label&quot;&gt;简介：&lt;/label&gt;
						    &lt;div class=&quot;col-sm-10&quot;&gt;
						      &lt;textarea rows=&quot;3&quot; placeholder=&quot;请输入自我简介&quot; class=&quot;form-control&quot;&gt;&lt;/textarea&gt;
						      &lt;div class=&quot;help-block&quot;&gt;简介不能为空！&lt;/div&gt;
						    &lt;/div&gt;
						  &lt;/div&gt;
						  &lt;div class=&quot;form-group&quot;&gt;
						    &lt;div class=&quot;col-sm-offset-2 col-sm-10&quot;&gt;
						      &lt;div class=&quot;checkbox&quot;&gt;
						        &lt;label&gt;
						          &lt;input type=&quot;checkbox&quot; id=&quot;agree&quot;&gt;
						          &lt;label for=&quot;agree&quot;&gt;&lt;/label&gt;&lt;a href=&quot;#&quot; class=&quot;link&quot;&gt;Agree the Protocol&lt;/a&gt;
						        &lt;/label&gt;
						      &lt;/div&gt;
						      &lt;div class=&quot;help-block&quot;&gt;请勾选同意协议！&lt;/div&gt;
						    &lt;/div&gt;
						  &lt;/div&gt;
						  &lt;div class=&quot;form-group&quot;&gt;
						    &lt;div class=&quot;col-sm-offset-2 col-sm-10&quot;&gt;
						      &lt;button type=&quot;submit&quot; class=&quot;oola-btn&quot;&gt;Submit&lt;/button&gt;
						    &lt;/div&gt;
						  &lt;/div&gt;
						&lt;/form&gt;
		section#pagination
